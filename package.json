{
  "name": "@ra-libs/nestjs",
  "version": "1.5.18",
  "description": "React admin nestjs lib",
  "author": "Rabah Zeineddine",
  "license": "MIT",
  "readmeFilename": "README.md",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "preinstall": "npx only-allow pnpm",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "prepare": "npm run build && husky",
    "format": "prettier --write \"src/**/*.ts\"",
    "prelint": "npm run format",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --coverage --maxWorkers=2 --detectOpenHandles --forceExit --passWithNoTests",
    "test:local": "jest --verbose --coverage --maxWorkers=8 --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "presemantic-release": "npm run build",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "nestjs",
    "react-admin",
    "ra"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.10",
    "@nestjs/core": "^10.3.10",
    "@nestjs/platform-express": "^10.3.10",
    "content-range": "^2.0.2",
    "flat": "^5.0.2",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "qs": "^6.12.2",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.8",
    "rxjs": "^7.8.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@nestjs/testing": "^10.3.10",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.7",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/express": "^4.17.21",
    "@types/flat": "^5.0.5",
    "@types/jest": "29.5.12",
    "@types/node": "^20.14.10",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^9.6.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.0.0",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "jest": "29.7.0",
    "prettier": "^2.8.8",
    "semantic-release": "^24.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "29.1.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ra-libs/nestjs"
  }
}